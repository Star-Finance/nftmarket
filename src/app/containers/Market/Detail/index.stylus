// #root,.lt-content
// overflow:auto
.page-marketDetail
    padding: 40px 0 0
    overflow: auto

    // .lt-header
    // background: #25252d
    ::-webkit-scrollbar
        width: 5px
        height: 5px

    ::-webkit-scrollbar-track-piece
        -webkit-border-radius: 6px
        background-color: rgba(0, 0, 0, 0.2)

    ::-webkit-scrollbar-thumb:vertical
        height: 5px
        -webkit-border-radius: 6px
        background-color: rgba(125, 125, 125, 0.7)

    ::-webkit-scrollbar-thumb:horizontal
        width: 5px
        -webkit-border-radius: 6px
        background-color: rgba(125, 125, 125, 0.7)

    .right-div-btn
        width: 500px
        padding: 30px 22px
        // border-left: 1px dashed rgba(255, 255, 255, 0.3)
        background: #25252D
        border-radius: 12px
        display: flex
        flex-direction: column
        justify-content: center

        .div_btn
            background: linear-gradient(24deg, #FF00E5 0%, #FFA200 100%)
            border-radius: 7px
            height: 50px
            font-size: 20px
            font-weight: 900
            display: flex
            justify-content: center
            align-items: center
            cursor: pointer
            border: 0

            &:hover
                background: linear-gradient(24deg, #FF42D5 0%, #FFA844 100%)
                // box-shadow: 0px 2px 14px 0px #FF00E5;
                // border-radius: 8px;

            &:active
                background: linear-gradient(24deg, #E023B6 0%, #E08926 100%)

        .ant-btn-primary[disabled], .ant-btn-primary[disabled]:active, .ant-btn-primary[disabled]:focus, .ant-btn-primary[disabled]:hover
            color: rgba(255, 255, 255, 0.45)
            border-color: #6A6A72
            background: #6A6A72
            text-shadow: none
            box-shadow: none

        .r-title
            display: flex
            align-items: center

            .icon
                width: 24px
                height: 24px
                margin-right: 24px

            span
                font-size: 18px
                font-weight: 900

    .nftImgDialog
        .ant-modal-content
            background-color: transparent
            min-width: 400px

            .modalMain
                text-align: center

                img
                    position: static !important
                    transform: translate(0, 0) !important

            .modalTitle
                position: relative
                color: #fff
                border-bottom: 0
                padding: 0 20px 20px

                em
                    position: absolute
                    top: -20px
                    right: -40px
                    width: 28px
                    height: 28px
                    display: block
                    background: url('../../../components/LogoutDialog/img/close.png') no-repeat
                    background-size: 100% 100%
                    text-indent: -999em
                    overflow: hidden
                    cursor: pointer

