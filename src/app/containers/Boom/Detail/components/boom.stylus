          position relative;
          top:-2rem
  .boom-box
     .img-open-loading
         display: none
    .approve
        background-image: url(../img/approve_bg.png)
        flexCenter()
        flex-direction: column
        padding:torem(65) 0 torem(80) 0
        .title
            font-size: torem(42);
            font-family: Avenir-Black, Avenir;
            font-weight: 900;
            color: #FFFFFF;
